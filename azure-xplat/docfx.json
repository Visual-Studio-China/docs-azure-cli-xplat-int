{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md","**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "azure-xplat-cli-poc/**",
          "**/includes/**",
          "specs/*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "breadcrumb.json"
        ],
        "exclude": [
          "**/obj/**",
          "azure-xplat-cli-poc/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": ["specs/*.md"],
    "globalMetadata": {
        "breadcrumb_path": "/azure-xplat-cli-poc/breadcrumb.json",
        "brand": "azure"
    },
    "dest": "azure-xplat-cli-poc"
  }
}